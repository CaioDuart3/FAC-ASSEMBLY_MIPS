.text
main:
    li $v0, 5
    syscall

    move $s0, $v0 
    add $s0, $s0, -1
    
    li $v0, 5  
    syscall
    move $t1, $v0
    
    loop:
        beq $s0, $zero, saida 

        li $v0, 5  
        syscall
        move $t0, $v0

        bge $t0, $t1, maior

        addi $s0, $s0, -1
        j loop


saida:
    li $v0, 1
    move $a0, $t1
    syscall

    li $v0, 10
    syscall

maior: 
    move $t1, $t0
    addi $s0, $s0, -1
    j loop