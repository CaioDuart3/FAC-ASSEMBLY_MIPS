.text:

#a1 = tamanho
#a0 = a[0]
.data:
main:
    li $t0, 0 #contador
    li $s0, 1 #i
    loop:
        beq $a1, $s0, saida  #enquanto tamanho != i

        mul $s1, $s0, 4 #i * 4bytes
        add $s1, $s1, $a0 #s1 = a[i]
        lw $s2, 0(s1)  # s2 = *s1
        bne $a0, ,$s2, contador

        move $a0, $s2 # a[i] = a[i+1]
        addi, $s0, $s0, 1 #i++
        j loop 

contador:
    addi $t0,$t0, 1
    addi, $s0, $s0, 1 #i++
    j loop 

saida: 
    li $v0, 10
    syscall